{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["cypress"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "cypress/component/Steam.cy.jsx", "cypress/component/Steam.cy.jsx", "cypress/component/steam.cy.js", "cypress/component/steam.cy.js", "cypress/component/Header.cy.js", "cypress/component/Header.cy.js", "cypress/component/Form.cy.js", "cypress/component/Form.cy.js"],
  "exclude": ["node_modules"]
}
